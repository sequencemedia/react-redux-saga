{
  "compact": true,
  "presets": [
    [
      "env", {
        "targets": {
          "node": "current",
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        },
        "useBuiltIns": true
      }
    ],
    "react",
    "stage-1"
  ],
  "plugins": [
    "transform-runtime",
    "transform-react-jsx",
    [
      "module-resolver", {
        "cwd": "babelrc",
        "alias": {
          "app": "./src/app"
        }
      }
    ]
  ]
}
